{
  "name": "gelm-webview",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "config": "^3.3.6",
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "ejs": "~3.1.7",
    "express": "~4.17.3",
    "express-validator": "^6.12.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.2",
    "morgan": "~1.9.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.13"
  }
}
