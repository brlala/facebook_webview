{
  "name": "gelm-webview",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "config": "^3.2.4",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "~3.0.1",
    "express": "~4.17.1",
    "express-validator": "^6.3.0",
    "http-errors": "~1.7.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.4",
    "morgan": "~1.9.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  }
}
